<tr class="nested-fields">
  <td><%= f.text_field :name, :class => "form-control" %></td>
  <td><%= f.text_field :repetitions, :class => "form-control", placeholder: 'Repetições' %></td>
  <td><%= f.text_field :series, :class => "form-control", placeholder: 'Séries' %></td>
  <td><%= f.text_field :interval, :class => "form-control", placeholder: 'Intervalo' %></td>
  <td><%= f.text_field :weight, :class => "form-control", placeholder: 'Peso' %></td>
  <td><%= link_to_remove_association "apagar exercício", f, class: 'btn btn-danger' %></td>
</tr>
